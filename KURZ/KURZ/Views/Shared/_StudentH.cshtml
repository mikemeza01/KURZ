                                <div class="dropdown-header"><strong>Cuenta</strong></div>
                                <a class="dropdown-item"
                                   asp-controller="Student" asp-action="EditAccount">Editar Cuenta</a>
                                <a class="dropdown-item"
                                   asp-controller="Authentication" asp-action="Logout">Cerrar Sesión</a>
                            </div>
                        </div>
                    </div>

                    <!-- // END Navbar Menu -->

                </div>

                <!-- // END Navbar -->

            </div>
        </div>

